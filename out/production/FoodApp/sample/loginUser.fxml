<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="240.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UserController">
    <children>
        <TextField fx:id="userText" layoutX="128.0" layoutY="53.0" prefHeight="46.0" prefWidth="210.0" promptText="Enter your username">
         <font>
            <Font size="18.0" />
         </font></TextField>
          <Button fx:id="loginButton" layoutX="152.0" layoutY="168.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="46.0" prefWidth="74.0" text="Login" />
          <Button fx:id="resetButton" layoutX="269.0" layoutY="168.0" mnemonicParsing="false" onAction="#resetAction" prefHeight="46.0" prefWidth="74.0" text="Reset" />
        <Label layoutX="21.0" layoutY="56.0" prefHeight="40.0" prefWidth="100.0" text="Username:">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Button layoutX="21.0" layoutY="168.0" mnemonicParsing="false" onAction="#backtoAdmin" prefHeight="46.0" prefWidth="74.0" text="Back" />
    </children>
</AnchorPane>
